{
  "name": "tree-sitter-godot-resource",
  "version": "0.1.0",
  "description": "tree-sitter parser for godot text resources (tres, tscn)",
  "main": "index.js",
  "keywords": [
    "lexer",
    "parser",
    "tres",
    "tscn"
  ],
  "author": "Preston Knopp",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.13.2",
    "prebuild-install": "^5.3.0"
  },
  "devDependencies": {
    "prebuild": "^8.2.1",
    "tree-sitter-cli": "^0.14.6"
  },
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "prebuild": "prebuild -r electron --all --strip --verbose",
    "prebuild:upload": "prebuild --upload-all",
    "build": "tree-sitter generate && tree-sitter build",
    "buildTest": "tree-sitter generate && tree-sitter build && tree-sitter test",
    "test": "tree-sitter test",
    "test-windows": "node_modules/.bin/tree-sitter test"
  },
  "repository": "https://github.com/PrestonKnopp/tree-sitter-godot-resource.git"
}
